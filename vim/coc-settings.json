{
  "suggest.autoTrigger": "none",
  "suggest.floatConfig": {
    "border": true,
    "rounded": true,
    "highlight": "on",
    // "borderhiglight": true,
    // "maxHeight": 15
  },
//  "[cpp][c]": {
//    "inlayHint": {
//      "Designators": false,
//      "Enabled": false,
